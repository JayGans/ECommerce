<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#fff"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    tools:context="my.edu.wings.homepage">

    <!-- TODO: Update blank fragment layout -->



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/milkynice"
                android:textSize="20dp"
                android:textStyle="bold"
                android:gravity="center"

                android:paddingLeft="20dp"
                android:textColor="#18191F"
                android:text="Test Your Knowledge"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
card_view:cardBackgroundColor="#BFCE10"
                    android:id="@+id/cardlevel1"
                    android:elevation="8dp"
                    android:divider="@android:color/transparent"

                    card_view:cardElevation="5dp"
                    card_view:cardUseCompatPadding="true"
                 >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="5dp"
                    android:background="#3efffdfd"
                    android:id="@+id/layptest"

                    android:layout_weight="1">
<ImageView
    android:layout_width="120dp"
    android:layout_height="100dp"
    android:background="@drawable/practice"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:fontFamily="@font/milkynice"
                        android:textSize="20dp"
                        android:textStyle="bold"
                        android:gravity="center_vertical"
                        android:id="@+id/crbal"
                        android:paddingLeft="20dp"
                        android:textColor="#ffffff"
                        android:text="Practice Test"/>

                </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"

                    card_view:cardBackgroundColor="#4CAF50"
                    android:elevation="8dp"
                    android:divider="@android:color/transparent"

                    card_view:cardUseCompatPadding="true"
                    android:id="@+id/carddepo"
                    card_view:cardElevation="5dp"
                   >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"

                        android:padding="5dp"
                        android:id="@+id/layallr"
                        android:background="#39fffdfd"
                        android:layout_weight="1">
                        <ImageView
                            android:layout_width="120dp"
                            android:layout_height="100dp"
                            android:background="@drawable/allrank"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:fontFamily="@font/milkynice"
                            android:textSize="20dp"
                            android:gravity="center_vertical"
                            android:textStyle="bold"
                            android:paddingLeft="20dp"
                            android:id="@+id/depoamt"
                            android:textColor="#ffffff"
                            android:text="All India Rank Level"/>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>






        </LinearLayout>

</FrameLayout>
