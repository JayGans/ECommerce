<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backq"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    tools:context="my.edu.wings.homepage">

    <!-- TODO: Update blank fragment layout -->


    <android.support.v7.widget.CardView
        android:layout_width="match_parent"

        android:layout_height="match_parent"

        card_view:cardBackgroundColor="#97FFFFFF"

        card_view:cardElevation="0dp"

        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"

                android:layout_height="wrap_content"

                card_view:cardBackgroundColor="#BFFFFFFF"


                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">
                    <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="4dp"
                        android:id="@+id/btback3"
                        android:layout_marginRight="7dp"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/ic_arrow_back"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/txt_title3"
                        android:textColor="#0C0E0C"
                        android:textStyle="normal"
                        android:drawablePadding="5dp"
                        android:layout_margin="4dp"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:fontFamily="@font/sf_ui_display_medium"
                        android:textSize="17dp"
                        android:padding="10dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0/0"
                        android:textColor="#0C0E0C"
                        android:textStyle="bold"
                        android:id="@+id/q_count3"
                        android:drawablePadding="5dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginBottom="4dp"
                        android:gravity="start"
                        android:fontFamily="@font/sf_ui_display_medium"
                        android:textSize="17dp"
                        android:padding="10dp"
                        />

                </LinearLayout>
            </android.support.v7.widget.CardView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="0dp"
                    android:background="@drawable/circle_boundry"
                    android:padding="3dp"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_check"/>
                </LinearLayout>
                <TextView
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:text="0"
                    android:id="@+id/txtearn3"
                    android:textColor="#0C0E0C"
                    android:textStyle="normal"
                    android:drawablePadding="5dp"
                    android:layout_margin="4dp"

                    android:gravity="center"
                    android:fontFamily="@font/sf_ui_display_medium"
                    android:textSize="14dp"
                    android:padding="0dp"
                    />
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_height="wrap_content">

                    <ProgressBar
                        android:id="@+id/progressBar3"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_centerInParent="true"
                        android:indeterminate="false"
                        android:max="100"
                        android:background="@drawable/circle_shape"
                        android:progress="0"
                        android:progressDrawable="@drawable/custom_progressbar_drawable"
                        android:secondaryProgress="0" />


                    <TextView
                        android:id="@+id/txtProgress3"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:background="@drawable/circle_blue"
                        android:layout_alignBottom="@+id/progressBar"
                        android:layout_centerInParent="true"
                        android:text="0"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:textColor="#FFFFFF"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/circle_boundry"
                    android:padding="3dp"
                    android:layout_marginLeft="15dp"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close_black"/>
                </LinearLayout>
                <TextView
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:text="0"
                    android:id="@+id/txtloss3"
                    android:textColor="#0C0E0C"
                    android:textStyle="normal"

                    android:layout_margin="4dp"
                    android:gravity="center"
                    android:fontFamily="@font/sf_ui_display_medium"
                    android:textSize="14dp"
                    android:padding="0dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="7dp"
                android:layout_margin="5dp"

                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="0"
                        android:id="@+id/txtright3"
                        android:fontFamily="@font/sf_ui_display_medium"
                        android:padding="1dp"
                        android:textSize="14dp"
                        android:layout_marginBottom="3dp"
                        android:textColor="#4CAF50"/>
                    <ProgressBar
                        style="@android:style/Widget.ProgressBar.Horizontal"
                        android:layout_centerInParent="true"
                        android:layout_width="15dp"
                        android:layout_height="match_parent"
                        android:max="100"
                        android:padding="4dp"
                        android:id="@+id/progress_right3"
                        android:background="@drawable/progress_back"
                        android:progress="0"
                        android:progressDrawable="@drawable/progress_vertical" />
                </LinearLayout>
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    card_view:cardCornerRadius="8dp"
                    card_view:cardBackgroundColor="#FFFFFF"


                    android:divider="@android:color/transparent"

                    card_view:cardElevation="2dp"
                    card_view:cardUseCompatPadding="true"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="130dp"
                            android:text=""
                            android:textColor="@color/colorPrimaryDark"
                            android:textStyle="bold"
                            android:scrollbars = "vertical"
                            android:gravity="center"
                            android:id="@+id/txt_queation3"
                            android:fontFamily="@font/sf_ui_display_medium"
                            android:textSize="14dp"
                            android:padding="5dp"
                            />
                        <ImageButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/ic_voice"
                            android:layout_marginRight="20dp"
                            android:id="@+id/btvoice3"
                            android:layout_marginBottom="3dp"
                            android:layout_gravity="right"/>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="0"
                        android:id="@+id/txtwrong3"
                        android:fontFamily="@font/sf_ui_display_medium"
                        android:padding="1dp"
                        android:textSize="14dp"
                        android:layout_marginBottom="3dp"
                        android:textColor="#F44336"/>
                    <ProgressBar
                        style="@android:style/Widget.ProgressBar.Horizontal"
                        android:layout_centerInParent="true"
                        android:layout_width="15dp"
                        android:layout_height="match_parent"
                        android:max="100"
                        android:padding="4dp"
                        android:id="@+id/progress_wrong3"
                        android:background="@drawable/progress_back_red"
                        android:progress="0"
                        android:progressDrawable="@drawable/progress_vertical_red" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:orientation="vertical">
                    <android.support.v7.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        card_view:cardCornerRadius="20dp"
                        android:id="@+id/card_a3"
                        card_view:cardBackgroundColor="#FFFFFF"
                        android:divider="@android:color/transparent"
                        card_view:cardUseCompatPadding="true"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"

                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="#1B1D25"
                                android:textStyle="normal"
                                android:drawableLeft="@drawable/a"
                                android:id="@+id/opt13"
                                android:layout_weight="1"
                                android:drawablePadding="7dp"
                                android:layout_margin="1dp"
                                android:padding="3dp"
                                android:gravity="center_vertical"
                                android:fontFamily="@font/sf_ui_display_medium"
                                android:textSize="13dp"

                                />
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginRight="2dp"
                                android:id="@+id/rel_a3"
                                android:visibility="gone"
                                android:layout_marginLeft="1dp"
                                android:layout_height="wrap_content">

                                <ProgressBar
                                    android:id="@+id/pro_a3"
                                    style="?android:attr/progressBarStyleHorizontal"
                                    android:layout_width="37dp"
                                    android:layout_height="37dp"
                                    android:layout_centerInParent="true"
                                    android:indeterminate="false"
                                    android:max="100"
                                    android:background="@drawable/circle_shape"
                                    android:progress="0"
                                    android:progressDrawable="@drawable/custom_progressbar_drawable"
                                    android:secondaryProgress="0" />


                                <TextView
                                    android:id="@+id/txt_a3"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:background="@drawable/circle_blue"
                                    android:layout_alignBottom="@+id/pro_a3"
                                    android:layout_centerInParent="true"
                                    android:text="0%"
                                    android:textSize="11dp"
                                    android:textStyle="bold"
                                    android:gravity="center"
                                    android:textColor="#FFFFFF"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />
                            </RelativeLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        card_view:cardCornerRadius="20dp"
                        android:id="@+id/card_b3"
                        card_view:cardBackgroundColor="#FFFFFF"
                        android:divider="@android:color/transparent"
                        card_view:cardUseCompatPadding="true"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="#1B1D25"
                                android:textStyle="normal"
                                android:drawableLeft="@drawable/b"
                                android:layout_weight="1"
                                android:id="@+id/opt23"
                                android:drawablePadding="7dp"
                                android:layout_margin="1dp"
                                android:padding="3dp"
                                android:gravity="center_vertical"
                                android:fontFamily="@font/sf_ui_display_medium"
                                android:textSize="13dp"

                                />
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginRight="2dp"
                                android:layout_marginLeft="1dp"
                                android:id="@+id/rel_b3"
                                android:visibility="gone"
                                android:layout_height="wrap_content">

                                <ProgressBar
                                    android:id="@+id/pro_b3"
                                    style="?android:attr/progressBarStyleHorizontal"
                                    android:layout_width="37dp"
                                    android:layout_height="37dp"
                                    android:layout_centerInParent="true"
                                    android:indeterminate="false"
                                    android:max="100"
                                    android:background="@drawable/circle_shape"
                                    android:progress="0"
                                    android:progressDrawable="@drawable/custom_progressbar_drawable"
                                    android:secondaryProgress="0" />


                                <TextView
                                    android:id="@+id/txt_b3"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:background="@drawable/circle_blue"
                                    android:layout_alignBottom="@+id/pro_b3"
                                    android:layout_centerInParent="true"
                                    android:text="0%"
                                    android:textSize="11dp"
                                    android:textStyle="bold"
                                    android:gravity="center"
                                    android:textColor="#FFFFFF"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />
                            </RelativeLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        card_view:cardCornerRadius="20dp"
                        android:id="@+id/card_c3"
                        card_view:cardBackgroundColor="#FFFFFF"
                        android:divider="@android:color/transparent"
                        card_view:cardUseCompatPadding="true"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="#1B1D25"
                                android:textStyle="normal"
                                android:drawableLeft="@drawable/c"
                                android:layout_weight="1"
                                android:id="@+id/opt33"
                                android:drawablePadding="7dp"
                                android:layout_margin="1dp"
                                android:padding="3dp"
                                android:gravity="center_vertical"
                                android:fontFamily="@font/sf_ui_display_medium"
                                android:textSize="13dp"

                                />
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginRight="2dp"
                                android:layout_marginLeft="1dp"
                                android:id="@+id/rel_c3"
                                android:visibility="gone"
                                android:layout_height="wrap_content">

                                <ProgressBar
                                    android:id="@+id/pro_c3"
                                    style="?android:attr/progressBarStyleHorizontal"
                                    android:layout_width="37dp"
                                    android:layout_height="37dp"
                                    android:layout_centerInParent="true"
                                    android:indeterminate="false"
                                    android:max="100"
                                    android:background="@drawable/circle_shape"
                                    android:progress="0"
                                    android:progressDrawable="@drawable/custom_progressbar_drawable"
                                    android:secondaryProgress="0" />


                                <TextView
                                    android:id="@+id/txt_c3"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:background="@drawable/circle_blue"
                                    android:layout_alignBottom="@+id/pro_c3"
                                    android:layout_centerInParent="true"
                                    android:text="0%"
                                    android:textSize="11dp"
                                    android:textStyle="bold"
                                    android:gravity="center"
                                    android:textColor="#FFFFFF"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />
                            </RelativeLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        card_view:cardCornerRadius="20dp"
                        android:id="@+id/card_d3"
                        card_view:cardBackgroundColor="#FFFFFF"
                        android:divider="@android:color/transparent"
                        card_view:cardUseCompatPadding="true"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="#1B1D25"
                                android:textStyle="normal"
                                android:drawableLeft="@drawable/d"
                                android:layout_weight="1"
                                android:drawablePadding="7dp"
                                android:layout_margin="1dp"
                                android:padding="3dp"
                                android:id="@+id/opt43"
                                android:gravity="center_vertical"
                                android:fontFamily="@font/sf_ui_display_medium"
                                android:textSize="13dp"

                                />
                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginRight="2dp"
                                android:layout_marginLeft="1dp"
                                android:id="@+id/rel_d3"
                                android:visibility="gone"
                                android:layout_height="wrap_content">

                                <ProgressBar
                                    android:id="@+id/pro_d3"
                                    style="?android:attr/progressBarStyleHorizontal"
                                    android:layout_width="37dp"
                                    android:layout_height="37dp"
                                    android:layout_centerInParent="true"
                                    android:indeterminate="false"
                                    android:max="100"
                                    android:background="@drawable/circle_shape"
                                    android:progress="0"
                                    android:progressDrawable="@drawable/custom_progressbar_drawable"
                                    android:secondaryProgress="0" />


                                <TextView
                                    android:id="@+id/txt_d3"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:background="@drawable/circle_blue"
                                    android:layout_alignBottom="@+id/pro_d3"
                                    android:layout_centerInParent="true"
                                    android:text="0%"
                                    android:textSize="11dp"
                                    android:textStyle="bold"
                                    android:gravity="center"
                                    android:textColor="#FFFFFF"
                                    android:textAppearance="?android:attr/textAppearanceSmall" />
                            </RelativeLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>


                </LinearLayout>

            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="7dp"
                android:paddingBottom="20dp"
                android:gravity="center_horizontal"

                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <TextView
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:text="50:50"
                    android:gravity="center"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/circle_with_shadow_blue"
                    android:textColor="#fff"
                    android:textSize="13dp"
                    android:id="@+id/bt50501"
                    android:layout_gravity="bottom"
                    android:fontFamily="@font/sf_ui_display_medium"/>

                <ImageButton
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:id="@+id/btshowpeople3"
                    android:layout_marginLeft="20dp"
                    android:gravity="center"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/circle_with_shadow"
                    android:textColor="#fff"
                    android:src="@drawable/ic_group"
                    android:textSize="13dp"
                    android:layout_gravity="bottom"
                    android:fontFamily="@font/sf_ui_display_medium"/>

                <TextView
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:text="SKIP"
                    android:id="@+id/practice_skipp3"
                    android:layout_marginLeft="20dp"
                    android:gravity="center"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/circle_with_shadow_yellow"
                    android:textColor="#fff"
                    android:textStyle="bold"
                    android:textSize="13dp"
                    android:layout_gravity="bottom"
                    android:fontFamily="@font/sf_ui_display_medium"/>


            </LinearLayout>


        </LinearLayout>

    </android.support.v7.widget.CardView>
</FrameLayout>
